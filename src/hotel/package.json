{
  "name": "exampletravel",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run prod",
    "server": "./node_modules/.bin/babel-node ./src/bin/www",
    "server:prod": "node ./dist/bin/www",
    "dev": "NODE_ENV=development ./node_modules/.bin/npm-run-all server",
    "clean": "./node_modules/.bin/rimraf dist",
    "test-suite": "babel-node test/run",
    "test": "./node_modules/.bin/npm-run-all test-suite",
    "build": "./node_modules/.bin/babel ./src --out-dir dist",
    "prod": "NODE_ENV=production npm-run-all clean build server:prod",
    "prod-build": "NODE_ENV=production ./node_modules/.bin/npm-run-all clean build"
  },
  "dependencies": {
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "swagger-ui-express": "^4.1.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/node": "^7.6.3",
    "@babel/preset-env": "^7.6.3",
    "babel-eslint": "^10.0.3",
    "npm-run-all": "^4.1.5",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "clean-css": ">=4.1.11",
    "constantinople": ">=3.1.1",
    "eslint": "^6.5.1",
    "mocha": "^6.2.1",
    "uglify-js": ">=2.6.0"
  }
}
