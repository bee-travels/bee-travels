{
  "name": "currencyexchange",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=production npm-run-all clean build server:prod",
    "dev": "babel-watch ./src/bin/www",
    "clean": "rimraf dist",
    "build": "babel ./src --out-dir dist",
    "server:prod": "node ./dist/bin/www",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint --ext .js ./src",
    "format": "prettier --write 'src/**/*.+(js|json)'"
  },
  "dependencies": {
    "acorn": "^7.1.0",
    "axios": "^0.19.0",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.5",
    "csvtojson": "^2.0.10",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "express-pino-logger": "^4.0.0",
    "http-errors": "~1.6.2",
    "jade": "~1.11.0",
    "npm-run-all": "^4.1.5",
    "swagger-ui-express": "^4.1.1",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/node": "^7.6.3",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/runtime": "^7.7.2",
    "babel-eslint": "^10.0.3",
    "babel-watch": "^7.0.0",
    "clean-css": ">=4.1.11",
    "constantinople": ">=3.1.1",
    "eslint": "^6.5.1",
    "husky": "^4.2.1",
    "jest": "^24.9.0",
    "lint-staged": "^10.0.2",
    "prettier": "^1.19.1",
    "uglify-js": ">=2.6.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.+(js|json)": "prettier --write"
  }
}
