{
  "name": "currencyexchange",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run prod",
    "dev": "NODE_ENV=development ./node_modules/.bin/npm-run-all server",
    "server": "./node_modules/.bin/babel-node ./src/bin/www",
    "clean": "./node_modules/.bin/rimraf dist",
    "build": "./node_modules/.bin/babel ./src --out-dir dist",
    "server:prod": "node ./dist/bin/www",
    "test": "jest",
    "test:watch": "jest --watch",
    "prod": "NODE_ENV=production npm-run-all clean build server:prod",
    "prod-build": "NODE_ENV=production ./node_modules/.bin/npm-run-all clean build",
    "lint": "./node_modules/.bin/eslint --ext .js ./src",
    "format": "./node_modules/.bin/prettier --write 'src/**/*.+(js|json)'"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "csvtojson": "^2.0.10",
    "express": "~4.16.0",
    "express-pino-logger": "^4.0.0",
    "http-errors": "~1.6.2",
    "pino": "^5.16.0",
    "swagger-ui-express": "^4.1.1",
    "yamljs": "^0.3.0",
    "@babel/runtime": "^7.8.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/node": "^7.6.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.6.3",
    "acorn": "^7.1.0",
    "babel-eslint": "^10.0.3",
    "clean-css": ">=4.1.11",
    "constantinople": ">=3.1.1",
    "eslint": "^6.5.1",
    "husky": "^4.2.1",
    "jest": "^24.9.0",
    "lint-staged": "^10.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "uglify-js": ">=2.6.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.+(js|json)": "prettier --write"
  }
}
